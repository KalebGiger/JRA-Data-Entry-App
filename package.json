{
  "name": "data-entry",
  "packageName": "Data Entry App",
  "version": "1.0.1",
  "license": "MIT",
  "description": "An app to quickly enter survey data.",
  "private": true,
  "author": "Kaleb Giger",
  "homepage": "./",
  "main": "src/start.js",
  "productName": "Data Entry App",
  "nsis": {
    "createDesktopShortcut": "always",
    "oneClick": false,
    "perMachine": false,
    "allowToChangeInstallationDirectory": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KalebGiger/JRA-Data-Entry-App.git"
  },
  "build": {
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "appId": "com.myapp.DataEntry",
    "publish": {
      "provider": "github",
      "repo": "JRA-Data-Entry-App",
      "owner": "KalebGiger"
    },
    "npmRebuild": false,
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.9.1",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.41",
    "electron-is-dev": "^1.1.0",
    "electron-json-storage": "^4.1.8",
    "electron-log": "^4.0.6",
    "electron-store": "^5.1.0",
    "electron-updater": "^4.2.0",
    "firebase": "^7.8.1",
    "grpc": "^1.23.3",
    "http2": "^3.3.7",
    "node": "^10.9.0",
    "node-pre-gyp": "^0.12.0",
    "notistack": "^0.9.7",
    "react": "^16.11.0",
    "react-csv": "^1.1.2",
    "react-dom": "^16.11.0",
    "react-scripts": "5.0.1",
    "react-with-firebase-auth": "^1.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "open": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "react-build": "react-scripts build",
    "dist-all": "electron-builder"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^8.0.0",
    "electron-builder": "^22.3.2"
  }
}
